<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>richwps</groupId>
    <artifactId>commons</artifactId>
    <packaging>pom</packaging>
    <name>richwps commons</name>
    <version>1.0.0-SNAPSHOT</version>
    <description>
        Components that can be used in different contexts.
    </description>
    <url>http://github.com/richwps/url</url>
    
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <version>2.5.1</version>
                    <configuration>
                        <source>1.6</source>
                        <target>1.6</target>
                        <encoding>${project.build.sourceEncoding}</encoding>
                    </configuration>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
    
    <modules>
        <module>common-xml</module>
        <module>wps-config</module>
        <module>wps-client-api</module>
    </modules>
  
  
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.52nwpsgroup>org.n52.wps</project.52nwpsgroup>
        <project.52nwpsversion>3.3.0-SNAPSHOT</project.52nwpsversion>
    </properties>
    
    <reporting>
        <plugins>
        </plugins>
    </reporting>
</project>
